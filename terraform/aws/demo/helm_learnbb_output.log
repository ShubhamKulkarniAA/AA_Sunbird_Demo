    output {
      kafka {
        bootstrap_servers => "kafka:9092"
        codec => plain {
            format => "%{message}"
        }
        message_key => "%{nodeUniqueId}"
        topic_id => "dev.knowlg.learning.graph.events"
        retries => 20
